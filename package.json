{
  "name": "agentic-planning-coordinator",
  "displayName": "Agentic Planning Coordinator",
  "description": "Multi-agent AI planning and execution system for Unity game development",
  "version": "0.5.0",
  "publisher": "GAOS",
  "author": {
    "name": "Yu Gao",
    "url": "https://www.linkedin.com/in/yugao-luckyvr/"
  },
  "homepage": "https://github.com/yugao-gaos/agentic-planning-coordinator",
  "repository": {
    "type": "git",
    "url": "https://github.com/yugao-gaos/agentic-planning-coordinator"
  },
  "bugs": {
    "url": "https://github.com/yugao-gaos/agentic-planning-coordinator/issues"
  },
  "license": "MIT",
  "icon": "resources/icon-256.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Programming Languages"
  ],
  "keywords": [
    "ai",
    "agents",
    "planning",
    "unity",
    "cursor",
    "automation",
    "game-development",
    "multi-agent"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agenticPlanning.showDashboard",
        "title": "Agentic: Show Dashboard"
      },
      {
        "command": "agenticPlanning.startPlanning",
        "title": "Agentic: Start Planning Session",
        "icon": "$(add)"
      },
      {
        "command": "agenticPlanning.startExecution",
        "title": "Start Execution",
        "icon": "$(play)"
      },
      {
        "command": "agenticPlanning.pauseExecution",
        "title": "Pause Execution",
        "icon": "$(debug-pause)"
      },
      {
        "command": "agenticPlanning.resumeExecution",
        "title": "Resume Execution",
        "icon": "$(play)"
      },
      {
        "command": "agenticPlanning.stopExecution",
        "title": "Stop Execution",
        "icon": "$(debug-stop)"
      },
      {
        "command": "agenticPlanning.showAgentTerminal",
        "title": "Agentic: Show Agent Terminal"
      },
      {
        "command": "agenticPlanning.poolStatus",
        "title": "Agentic: Show Agent Pool Status"
      },
      {
        "command": "agenticPlanning.refreshAgentPool",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "apc.openRoleSettings",
        "title": "APC: Configure Agent Roles",
        "icon": "$(gear)"
      },
      {
        "command": "apc.openDaemonSettings",
        "title": "APC: Daemon Settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "agenticPlanning.refreshPlanningSessions",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "agenticPlanning.refreshDependencies",
        "title": "Refresh Dependencies",
        "icon": "$(refresh)"
      },
      {
        "command": "agenticPlanning.openDependencyInstall",
        "title": "Install Dependency"
      },
      {
        "command": "agenticPlanning.copyDependencyCommand",
        "title": "Copy Install Command"
      },
      {
        "command": "agenticPlanning.installCli",
        "title": "Agentic: Install CLI (apc command)"
      },
      {
        "command": "agenticPlanning.uninstallCli",
        "title": "Agentic: Uninstall CLI"
      },
      {
        "command": "agenticPlanning.retryFailedTask",
        "title": "Retry Failed Task",
        "icon": "$(refresh)"
      },
      {
        "command": "agenticPlanning.stopPlanningSession",
        "title": "Stop Planning",
        "icon": "$(debug-stop)"
      },
      {
        "command": "agenticPlanning.resumePlanningSession",
        "title": "Resume Planning",
        "icon": "$(debug-start)"
      },
      {
        "command": "agenticPlanning.removePlanningSession",
        "title": "Remove Session",
        "icon": "$(trash)"
      },
      {
        "command": "agenticPlanning.revisePlan",
        "title": "Revise Plan",
        "icon": "$(edit)"
      },
      {
        "command": "agenticPlanning.approvePlan",
        "title": "Approve & Execute",
        "icon": "$(check)"
      },
      {
        "command": "agenticPlanning.releaseAgent",
        "title": "Release Agent",
        "icon": "$(debug-stop)"
      },
      {
        "command": "agenticPlanning.killStuckProcesses",
        "title": "Agentic: Kill Stuck Processes"
      },
      {
        "command": "agenticPlanning.showRunningProcesses",
        "title": "Agentic: Show Running Processes"
      },
      {
        "command": "agenticPlanning.openSettings",
        "title": "Open Settings",
        "icon": "$(gear)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agentic-planning",
          "title": "Agentic Planning",
          "icon": "resources/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "agentic-planning": [
        {
          "type": "webview",
          "id": "agenticPlanning.sidebarView",
          "name": "",
          "visibility": "visible"
        }
      ]
    },
    "configuration": {
      "title": "Agentic Planning Coordinator",
      "properties": {
        "agenticPlanning.workingDirectory": {
          "type": "string",
          "default": "_AiDevLog",
          "description": "Working directory for plans, logs, and state files"
        },
        "agenticPlanning.defaultBackend": {
          "type": "string",
          "default": "cursor",
          "enum": [
            "cursor"
          ],
          "description": "AI backend for agent sessions (only 'cursor' is currently supported)"
        },
        "agenticPlanning.autoOpenTerminals": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open terminal for each agent when started"
        }
      }
    },
    "menus": {
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  }
}
